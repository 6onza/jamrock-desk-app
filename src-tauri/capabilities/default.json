{
  "identifier": "default",
  "description": "Default capabilities for Jamrock Admin",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "store:default",
    "shell:default",
    "dialog:default",
    "fs:default",
    {
      "identifier": "fs:allow-write-file",
      "allow": [{ "path": "$TEMP/**" }]
    },
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$TEMP/**" }]
    },
    "notification:default",
    "process:default",
    "opener:default",
    "updater:default"
  ]
}
